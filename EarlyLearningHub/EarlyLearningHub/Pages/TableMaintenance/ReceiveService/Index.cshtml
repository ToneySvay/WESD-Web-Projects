@page
@model EarlyLearningHub.Pages.TableMaintenance.ReceiveService.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ReceiveService[0].RsOptions)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ReceiveService) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RsOptions)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.RsId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.RsId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.RsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
