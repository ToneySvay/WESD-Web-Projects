@page
@model EarlyLearningHub.Pages.TableMaintenance.HomelessStatus.IndexModel

@{
    ViewData["Title"] = "Homeless Status";
}

@*<h2>Index</h2>*@

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HomelessStatus[0].HsOptions)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.HomelessStatus)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.HsOptions)
                </td>
                <td>
                    <a asp-page="./Edit" asp-route-id="@item.HsId">Edit</a> |
                    <a asp-page="./Details" asp-route-id="@item.HsId">Details</a> |
                    <a asp-page="./Delete" asp-route-id="@item.HsId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
